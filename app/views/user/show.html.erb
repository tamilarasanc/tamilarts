<% debugger %>

<div class="tab-pane" id="Registration">

  <%= form_for :user , :url => '/user' do |f| %>

      <%= f.label :name , 'Name' %> <%= f.text_field :name , class:'form-control', placeholder:'Name'  %>

      <%= f.label :email, 'Email' %> <%= f.text_field :email , class:'form-control', placeholder:'Email'  %>

      <%= f.label :mobile, 'Mobile' %><%= f.text_field :mobile_number , class:'form-control', placeholder:'Mobile'  %>

      <%= f.label :password, 'Password' %><%= f.password_field :password , class:'form-control', placeholder:'Password'  %>
      <% end %>
      <!--<input type="password" class="form-control" id="password" placeholder="Password" />-->
</div>